CREATE TABLE testschema.student (
    studentid INT NOT NULL PRIMARY KEY,
    fname VARCHAR(255) NOT NULL,
    lname VARCHAR(255) NOT NULL
);

ALTER TABLE testschema.student ALTER COLUMN studentid BIGINT;

ALTER TABLE testschema.student
ADD fname VARCHAR(255)
ADD lname VARCHAR(255);

DROP TABLE testschema.students;

