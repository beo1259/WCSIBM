ALTER TABLE testschema.student DROP COLUMN studentid;
ALTER TABLE testschema.student ADD fname VARCHAR(255) NOT NULL ADD lname VARCHAR(255) NOT NULL;

REORG TABLE testschema.student;

ALTER TABLE testschema.student ALTER COLUMN fname DROP NOT NULL;


